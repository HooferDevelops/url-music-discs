{
	"schemaVersion": 1,
	"id": "urlmusicdiscs",
	"version": "${version}",
	"name": "URL Music Discs",
	"description": "Adds a inscribable music disc, allowing you to customize the music that plays.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/urlmusicdiscs/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"urlmusicdiscs.URLMusicDiscs"
		],
		"client": [
			"urlmusicdiscs.URLMusicDiscsClient"
		]
	},
	"mixins": [
		"urlmusicdiscs.mixins.json",
		{
			"config": "urlmusicdiscs.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}